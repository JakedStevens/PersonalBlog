@{
    ViewData["Title"] = "User Page";
    @model PersonalBlog.Web.ViewModels.LoginRegisterViewModel
}

<div class="auth-container">
    <div class="row">

        <partial name="_Alert" model="@Model.Alert" />

        <div class="col-12 col-md-6">
            <h1 class="my-font">Sign In</h1>
            <partial name="_Login" model="@Model.UserLogin" />
        </div>

        <div class="col-12 col-md-6">
            <h1 class="my-font">Register</h1>
            <partial name="_Register" model="@Model.UserRegister" />
        </div>

    </div>
</div>
